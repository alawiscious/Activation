{
  "version": 2,
  "name": "activation",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_DATA_SOURCE": "github",
    "VITE_DEFAULT_MASTER_CSV": "https://raw.githubusercontent.com/alawiscious/Activation/main/Public/Master%20Import%20file.csv",
    "VITE_GENOME_STORAGE_TYPE": "github",
    "VITE_GENOME_FALLBACK_LOCAL": "true"
  }
}
